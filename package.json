    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "jsx-a11y/anchor-is-valid": "off",
      "jsx-a11y/anchor-has-content": "off",
      "no-script-url": "off",
      "no-useless-escape": "off",
      "jsx-a11y/alt-text": "off",
      "react/jsx-no-target-blank": "off",
      "no-template-curly-in-string": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "@babel/syntax-export-extensions",
      [
        "prismjs",
        {
          "languages": [
            "c",
            "cpp",
            "java"
          ],
          "plugins": [
            "line-numbers"
          ],
          "theme": null,
          "css": null
        }
      ]
    ]
  },
  "proxy": {
    "/api/rule_service/v3": {
      "target": "http://127.0.0.1:3003/",
      "pathRewrite": {
        "^/api/rule_service/v3": ""
      }
    },
    "/api": {
      "target": "http://127.0.0.1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "stylelint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
  "jestWebpackResolver": {
    "webpackConfig": "./config/webpack.config.js"
  }
